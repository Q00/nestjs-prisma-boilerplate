    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "gts": "^1.1.2",
    "jest": "^25.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
    "fix": "gts fix",
    "dev": "NODE_ENV=dev nodemon",
    "start": "NODE_ENV=prod node dist/main.js",
    "start:dev": "NODE_ENV=dev node dist/main.js",
    "build": "nest build --path tsconfig.build.json",
    "postinstall": "npx prisma generate",
    "db:save:dev": "dotenv -e prisma/.env.dev -- npx prisma migrate save --experimental",
    "db:migration:dev": "dotenv -e prisma/.env.dev -- npx prisma migrate up --experimental",
    "db:save:prod": "dotenv -e prisma/.env.prod -- npx prisma migrate save --experimental",
    "db:migration:prod": "dotenv -e prisma/.env.prod -- npx prisma migrate up --experimental"
  },
  "dependencies": {
